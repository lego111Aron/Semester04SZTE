<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar layoutX="0.0" layoutY="7.0" prefHeight="27.0" prefWidth="Infinity" fx:controller="hu.alkfejl.view.common.MenuController" xmlns="http://javafx.com/javafx/13" xmlns:fx="http://javafx.com/fxml/1">
    <menus>
        <Menu mnemonicParsing="false" text="Jegyzőkönyv">
            <items>
                <MenuItem fx:id="jegyzokonyvFelvitelMI" mnemonicParsing="false" onAction="#openJegyzokonyvFelvitel" text="Felvitel" />
                <MenuItem fx:id="jegyzokonyvListazasMI" mnemonicParsing="false" onAction="#setJegyzokonyvListazas" text="Listázás" />
            </items>
        </Menu>
        <Menu  mnemonicParsing="false" text="Segítség">
            <MenuItem fx:id="segitsegMI" mnemonicParsing="false" onAction="#openSegitseg" text="Súgó"/>
        </Menu>

    </menus>
</MenuBar>

